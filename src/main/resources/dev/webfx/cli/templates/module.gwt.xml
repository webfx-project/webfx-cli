    <set-configuration-property name="includeSourceMapUrl" value="symbolMaps/__HASH___sourceMap__FRAGMENT__.json"/>
    <!-- Forcing installCode = true to make PWA offline working when style = PRETTY (i.e., gwt-dev Maven profile enabled) -->
    <set-configuration-property name="installCode" value="true" />

    <!-- Source paths section generated by WebFX -->

    <!-- Speeding up GWT compilation and execution times -->
    <set-property name="user.agent" value="safari"/> <!-- Only one permutation to speed up GWT compilation - now workings in all modern browsers -->
    <set-configuration-property name="user.agent.runtimeWarning" value="false"/> <!-- removing irrelevant warning in non-safari browsers -->
    <set-property name="jre.checks.checkLevel" value="MINIMAL"/> <!-- to speed up execution time -->

    <!-- Specifying the app entry point class. -->
    <entry-point class="dev.webfx.platform.boot.gwt.GwtEntryPoint"/>

    <!-- Including super sources possibly generated by the WebFX CLI -->
    <super-source path="super"/>

    <!-- Including the public folder that contains source web files such as index.html -->
    <public path="public"/>

    <!-- Including the possible resources related to CSS (fonts, images, etc...) -->
    <resource path="dev/webfx/kit/css"/>

</module>